{
  "manifest_version": 3,
  "name": "Awesome Chrome extensions by NamLN",
  "description": "Some extensions for Google Chrome developed by NamLN",
  "version": "1.0.1",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": ["tabs", "storage"],
  "action": {
    "default_title": "Awesome Chrome Extensions",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  }
}
